[31m-	|   └── head.html # html文档的头部内容[m
[31m-	|   └── header.html # 顶部菜单栏[m
[31m-	|   └── pageNav.html # 文章列表分页组件[m
[31m-	├── _layouts # 布局模板[m
[31m-	|   ├── default.html # 默认模板[m
[31m-	|   └── post.html # 文章页面模板[m
[31m-	├── _posts # 这里放文章[m
[31m-	|   ├── 2017-05-03-elements-of-javascript-style.md # 命名格式：年-月-日-文章标题.md[m
[31m-	|   └── 2007-02-21-life-on-mars.md[m
[31m-	├── _site # Jekyll将源码处理后生成的站点文件，里面的内容可直接发布[m
[31m-	├── assets # 存放用于线上环境的静态资源，如需修改css和js文件请到dev文件夹[m
[31m-	|   ├── css # dev文件夹中sass编译后的样式文件[m
[31m-	|   └── fonts # 字体文件[m
[31m-	|   └── icons # 图标文件[m
[31m-	|   └── img #  图片文件[m
[31m-	|   └── js # dev文件夹中处理后的脚本文件[m
[31m-	├── dev # 开发文件[m
[31m-	|   ├── js # 存放脚本源码[m
[31m-	|   └── sass # 样式源码[m
[31m-	|       └── app.scss # 整合下面的所有样式文件[m
[31m-	|       └── base.scss # 引入字体、Reset部分样式[m
[31m-	|       └── common.scss # 模板的主要样式[m
[31m-	|       └── helper.scss # 工具样式[m
[31m-	|       └── layouts.scss # 响应式布局[m
[31m-	└── gulpfile.js # 自动化任务脚本[m
[31m-	└── index.html # 模板首页[m
[31m-	└── tags.html # 标签页面[m
[31m-	└── 404.html # 404页面[m
[31m-	└── package.json # 管理项目的依赖项[m
[31m-```[m
